            "tsConfig": "apps/mobile/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            },
            "pre": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.pre.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "mobile:build",
            "serverTarget": "mobile:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "mobile:build:production",
              "serverTarget": "mobile:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "mobile:build:production",
            "serverTarget": "mobile:server:production",
            "routes": ["/"]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
    "mobile-e2e": {
      "root": "apps/mobile-e2e",
      "sourceRoot": "apps/mobile-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mobile-e2e/cypress.json",
            "tsConfig": "apps/mobile-e2e/tsconfig.e2e.json",
