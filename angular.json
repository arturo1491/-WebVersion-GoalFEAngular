          },
          "configurations": {
            "production": {
              "devServerTarget": "mobile:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mobile-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "comments": {
      "projectType": "library",
      "root": "libs/comments",
      "sourceRoot": "libs/comments/src",
      "prefix": "goal-front",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/comments/src/**/*.ts",
              "libs/comments/src/**/*.html"
            ]
          }
        },
        "test": {
